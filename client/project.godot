config_version=5

[application]

config/name="PowerGrid"
config/description="Online multiplayer Power Grid board game"
run/main_scene="res://src/scenes/main_menu/MainMenu.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")

[autoload]

NetworkManager="*res://src/autoload/NetworkManager.gd"
GameState="*res://src/autoload/GameState.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"

[rendering]

renderer/rendering_method="forward_plus"
